    "rollup": "^2.47.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.38.2",
    "svelte-check": "^1.5.2",
    "svelte-dts": "^0.3.2",
    "svelte-preprocess": "^4.7.3",
