  "version": "2.1.6",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdeneef/svelte-gitcal.git"
  },
  "author": "J de Neef",
  "bugs": {
    "url": "https://github.com/jdeneef/svelte-gitcal/issues"
